<resources>

<properties>
    <property id="hair" type="number">0</property>
    <property id="character" type="number">0</property>
    <property id="hands" type="number">0</property>
    <property id="candy" type="number">0</property>
</properties>

<settings>
  <setting propertyKey="@Properties.hair" title="@Strings.hairTitle" prompt="@Strings.hairPrompt">
    <settingConfig type="list">
       <listEntry value="0">@Strings.PonyTail</listEntry>
        <listEntry value="1">@Strings.CandyHair</listEntry>
        <listEntry value="2">@Strings.BoneHair</listEntry>
        <listEntry value="3">@Strings.BunHair</listEntry>
    </settingConfig>
  </setting>

 <setting propertyKey="@Properties.hands" title="@Strings.handsTitle" prompt="@Strings.handsPrompt">
    <settingConfig type="list">
       <listEntry value="0">@Strings.NormalHands</listEntry>
        <listEntry value="1">@Strings.FunHands</listEntry>
        <listEntry value="2">@Strings.WaveHands</listEntry>
        <listEntry value="3">@Strings.NoHands</listEntry>
    </settingConfig>
  </setting>

   <setting propertyKey="@Properties.character" title="@Strings.characterTitle" prompt="@Strings.characterPrompt">
    <settingConfig type="list">
       <listEntry value="0">@Strings.GhostGirl</listEntry>
        <listEntry value="1">@Strings.SomeCandy</listEntry>
    </settingConfig>
  </setting>

 <setting propertyKey="@Properties.candy" title="@Strings.candyTitle" prompt="@Strings.candyPrompt">
    <settingConfig type="list">
       <listEntry value="0">@Strings.TooMuchCandy</listEntry>
        <listEntry value="1">@Strings.SomeCandy</listEntry>
        <listEntry value="2">@Strings.NoCandy</listEntry>
  </settingConfig>
  </setting>

</settings>
</resources>
